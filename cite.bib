@article{gonzalez-fernandez2011,
  title = {Source and {{Fate}} of {{Heavy Metals}} in {{Marine Sediments}} from a {{Semi-Enclosed Deep Embayment Subjected}} to {{Severe Anthropogenic Activities}}},
  author = {González-Fernández, Daniel and Garrido-Pérez, M. Carmen and Nebot-Sanz, Enrique and Sales-Márquez, Diego},
