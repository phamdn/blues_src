@article{faggio2018,
  title = {Mussel Digestive Gland as a Model Tissue for Assessing Xenobiotics: {{An}} Overview},
  shorttitle = {Mussel Digestive Gland as a Model Tissue for Assessing Xenobiotics},
  author = {Faggio, Caterina and Tsarpali, Vasiliki and Dailianis, Stefanos},
