@article{demarchi2018,
  title = {The Influence of Salinity on the Effects of {{Multi-walled}} Carbon Nanotubes on Polychaetes},
  author = {De Marchi, Lucia and Neto, Victor and Pretti, Carlo and Figueira, Etelvina and Chiellini, Federica and Morelli, Andrea and Soares, Amadeu M. V. M. and Freitas, Rosa},
  date = {2018-06-05},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {8},
