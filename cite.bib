  keywords = {Carryover effect,Contaminants,Copepod fitness,Cumulative impact,Marine heatwave,Multiple stressors},
  file = {C\:\\Users\\nghia\\Zotero\\storage\\3XPX5PDC\\Dinh et al. - 2020 - Interactive effects of extreme temperature and a w.pdf;C\:\\Users\\nghia\\Zotero\\storage\\RSDZMAM3\\S0025326X20306275.html}
}

