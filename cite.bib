@article{loring1991,
  title = {Normalization of Heavy-Metal Data from Estuarine and Coastal Sediments},
  author = {Loring, D. H.},
  date = {1991-05-01},
  journaltitle = {ICES Journal of Marine Science},
  shortjournal = {ICES Journal of Marine Science},
