  title = {Integrated Biomarker Responses in Oysters {{Crassostrea}} Gasar as an Approach for Assessing Aquatic Pollution of a {{Brazilian}} Estuary},
  author = {Ferreira, Clarissa P. and Piazza, Thiago B. and Souza, Patrick and Lima, Daína and Mattos, Jacó J. and Saldaña-Serrano, Miguel and Piazza, Rômi S. and Jorge, Marianna B. and Bianchini, Adalto and Taniguchi, Satie and Sasaki, Silvio T. and Montone, Rosalinda C. and Bícego, Márcia C. and Bainy, Afonso C. D. and Lüchmann, Karim H.},
