@article{michaud2005,
  ids = {michaud2005a},
  title = {The Functional Group Approach to Bioturbation: {{The}} Effects of Biodiffusers and Gallery-Diffusers of the {{Macoma}} Balthica Community on Sediment Oxygen Uptake},
