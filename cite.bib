@article{beliaeff2002,
  title = {Integrated Biomarker Response: {{A}} Useful Tool for Ecological Risk Assessment},
  shorttitle = {Integrated Biomarker Response},
  author = {Beliaeff, Benoit and Burgeot, Thierry},
  date = {2002},
  journaltitle = {Environmental Toxicology and Chemistry},
  volume = {21},
  number = {6},
  pages = {1316--1322},
  issn = {1552-8618},
  doi = {10.1002/etc.5620210629},
  url = {https://setac.onlinelibrary.wiley.com/doi/abs/10.1002/etc.5620210629},
  urldate = {2021-03-12},
  abstract = {A battery of biomarkers is often used to evaluate the effects of exposure to chemical contaminants and detect responses to environmental stress. Unfortunately, field application of biomarkers is subject to various constraints (e.g., the availability of living material) that can limit data acquisition and prevent the use of multivariate methods during statistical analysis. In these circumstances, a simple method is needed to summarize biomarker responses and simplify their interpretation in biomonitoring programs. The present study used star plots to display results for the panel of biomarkers used for each station and survey. Integrated biomarker response (IBR) was then computed as the star plot area. Star plots using IBR values instead of biomarker data make it possible to visualize between-site and/or between-survey differences for comparison with exposure conditions. This approach was applied to sites in the Baltic Sea and the Seine Estuary, English Channel. In both cases, IBR values were visually compared to polycyclic aromatic hydrocarbons (PAH) or polychlorobiphenyls (PCB) levels measured in mussel or fish tissues. The IBR, as an indicator of environmental stress, appears to be a useful tool for scientists and managers in assessing ecological risk.},
  langid = {english},
  keywords = {Biomarkers,Coastal waters,Graphic tool,Mytilus edulis,Platichthys flesus},
  annotation = {662 citations (Crossref) [2022-08-26] \_eprint: https://setac.onlinelibrary.wiley.com/doi/pdf/10.1002/etc.5620210629},
  file = {C\:\\Users\\nghia\\Zotero\\storage\\2HA2MM79\\Beliaeff and Burgeot - 2002 - Integrated biomarker response A useful tool for e.pdf;C\:\\Users\\nghia\\Zotero\\storage\\29I533Q7\\etc.html}
