@article{biscere2017,
  title = {Nickel and Ocean Warming Affect Scleractinian Coral Growth},
  author = {Biscéré, T. and Lorrain, A. and Rodolfo-Metalpa, R. and Gilbert, A. and Wright, A. and Devissi, C. and Peignon, C. and Farman, R. and Duvieilbourg, E. and Payri, C. and Houlbrèque, F.},
