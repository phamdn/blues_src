@article{dagnino2007,
  title = {Development of an Expert System for the Integration of Biomarker Responses in Mussels into an Animal Health Index},
  author = {Dagnino, A. and Allen, J. I. and Moore, M. N. and Broeg, K. and Canesi, L. and Viarengo, A.},
  date = {2007-01-01},
  journaltitle = {Biomarkers},
  volume = {12},
