  issn = {1537-2537},
  doi = {10.2134/jeq2000.00472425002900040014x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.2134/jeq2000.00472425002900040014x},
  urldate = {2022-09-17},
  abstract = {Bioavailable heavy metals have been widely studied in recent years because of their toxicity to natural ecosystems. Despite the number of studies carried out, relatively little is known about the metal fraction that is biologically available. In this study, we analyzed seasonal concentrations of acid-volatile sulfide (AVS) as well as heavy metals associated with the reactive (soluble in 1 M HCl) and pyrite (soluble in HNO3) fractions of the soils of three salt marshes in the ría of Ortigueira (NW Iberian Peninsula). Heavy metal concentrations in polychaete worms (Nereis diversicolor) collected from each salt marsh were also determined and compared with metal contents in the different fractions of the soil. We did not find a relationship between the concentration of total metal or metal incorporated into the reactive fraction in the soil and the tissue-metal concentration in N. diversicolor. The results appear to indicate that this species preferentially accumulates metals associated with reduced forms of sulfur (pyrite and presumably AVS). Oxidation of these metal sulfides would lead to release of Cu and Ni into interstitial water and they would then be readily available to be taken up by these organisms, either by absorption through the body surface or across the digestive tract.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.2134/jeq2000.00472425002900040014x},
  file = {C\:\\Users\\nghia\\Zotero\\storage\\CHR8TAP5\\Otero et al. - 2000 - Bioaccumulation of Heavy Metals in Thionic Fluviso.pdf;C\:\\Users\\nghia\\Zotero\\storage\\KZLUFN7X\\Otero et al. - 2000 - Bioaccumulation of Heavy Metals in Thionic Fluviso.pdf;C\:\\Users\\nghia\\Zotero\\storage\\YRMGTD3Q\\jeq2000.html}
}

