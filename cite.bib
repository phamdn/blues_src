@article{dinh2020,
  title = {Interactive Effects of Extreme Temperature and a Widespread Coastal Metal Contaminant Reduce the Fitness of a Common Tropical Copepod across Generations},
  author = {Dinh, Khuong V. and Nguyen, Quyen T. T. and Vo, Thi-My-Chi and Bui, Trung Ba and Dao, Thanh-Son and Tran, Duc M. and Doan, Nam X. and Truong, Trinh S. H. and Wisz, Mary S. and Nielsen, Torkel Gissel and Vu, Minh T. T. and Le, Minh-Hoang},
  date = {2020-10-01},
  journaltitle = {Marine Pollution Bulletin},
