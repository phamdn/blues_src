@article{ernst2009,
  title = {Teaching {{Inference}} for {{Randomized Experiments}}},
  author = {Ernst, Michael D.},
  date = {2009-03-01},
  journaltitle = {Journal of Statistics Education},
  volume = {17},
  number = {1},
  pages = {null},
