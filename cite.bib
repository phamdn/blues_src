@article{hassler2004,
  title = {Some Fundamental (and Often Overlooked) Considerations Underlying the Free Ion Activity and Biotic Ligand Models},
  author = {Hassler, Christel S. and Slaveykova, Vera I. and Wilkinson, Kevin J.},
  date = {2004},
  journaltitle = {Environmental Toxicology and Chemistry},
  volume = {23},
