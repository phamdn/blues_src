@article{luo2020,
  title = {Rethinking {{Nano-TiO2 Safety}}: {{Overview}} of {{Toxic Effects}} in {{Humans}} and {{Aquatic Animals}}},
  shorttitle = {Rethinking {{Nano-TiO2 Safety}}},
  author = {Luo, Zhen and Li, Zhuoqing and Xie, Zhe and Sokolova, Inna M. and Song, Lan and Peijnenburg, Willie J. G. M. and Hu, Menghong and Wang, Youji},
  date = {2020},
