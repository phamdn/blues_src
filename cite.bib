  author = {Michaud, Emma and Desrosiers, Gaston and Mermillod-Blondin, Florian and Sundby, Bjorn and Stora, Georges},
  date = {2005-12-06},
  journaltitle = {Journal of Experimental Marine Biology and Ecology},
