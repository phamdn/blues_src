@article{babich1985,
  title = {The Mediation of Mutagenicity and Clastogenicity of Heavy Metals by Physicochemical Factors},
  author = {Babich, H. and Devanas, M. A. and Stotzky, G.},
  date = {1985-08-01},
  journaltitle = {Environmental Research},
  shortjournal = {Environmental Research},
  volume = {37},
