}

@article{dasilvafonseca2019,
  title = {Energy Metabolism Enzymes Inhibition by the Combined Effects of Increasing Temperature and Copper Exposure in the Coral {{Mussismilia}} Harttii},
  author = {da Silva Fonseca, Juliana and de Barros Marangoni, Laura Fernandes and Marques, Joseane Aparecida and Bianchini, Adalto},
  options = {useprefix=true},
  date = {2019-12-01},
  journaltitle = {Chemosphere},
