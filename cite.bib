@article{toussaint2021,
  title = {Faunal and Environmental Drivers of Carbon and Nitrogen Cycling along a Permeability Gradient in Shallow {{North Sea}} Sediments},
  author = {Toussaint, Elise and De Borger, Emil and Braeckman, Ulrike and De Backer, Annelies and Soetaert, Karline and Vanaverbeke, Jan},
  date = {2021-05-01},
  journaltitle = {Science of The Total Environment},
