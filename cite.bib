@article{thit2015,
  title = {Bioaccumulation, Subcellular Distribution and Toxicity of Sediment-Associated Copper in the Ragworm {{Nereis}} Diversicolor: {{The}} Relative Importance of Aqueous Copper, Copper Oxide Nanoparticles and Microparticles},
